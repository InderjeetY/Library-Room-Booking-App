<%= form_for(booking) do |f| %>
    <% if booking.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(booking.errors.count, "error") %> prohibited this user from being saved:</h2>

          <ul>
            <% booking.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <div class="field">
      <%= f.label :from_time %>
      <%= f.datetime_select :from_time%>
    </div>

    <div class="field">
      <%= f.label :to_time %>
      <%= f.datetime_select :to_time%>
    </div>

    <div class="field">
      <%= f.label :user_id %>
      <%= f.text_field :user_id, :readonly => true %>
    </div>

    <div class="field">
      <%= f.label :room_id %>
      <%= f.text_field :room_id, :readonly => true %>
    </div>

    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
